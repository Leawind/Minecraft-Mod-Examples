{
  "schemaVersion": 1,
  "id": "example_mod",
  "version": "${version}",
  "name": "Example mod",
  "description": "${mod_description}",
  "authors": [],
  "contact": {
    "homepage": "${mod_homepage_url}",
    "sources": "${mod_source_url}"
  },
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.github.username.examplemod.fabric.ExampleModClientFabric"
    ],
    "server": [
      "com.github.username.examplemod.fabric.ExampleModServerFabric"
    ]
  },
  "mixins": [
    "example_mod.mixins.json",
    "example_mod-common.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${minecraft_version}"
  },
  "suggests": {
  }
}
